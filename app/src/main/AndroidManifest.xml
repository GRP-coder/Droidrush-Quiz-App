<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.droidrush_quiz_app">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/logor"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logor"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Admin.AdminControl"
            android:theme="@style/AppTheme.notitle">
        </activity>


        <activity android:name=".Auth.TeacherLogIn"
            android:theme="@style/AppTheme.notitle"/>

        <activity android:name=".Student.Quizcode"
            android:theme="@style/AppTheme.notitle" />


        <activity android:name=".Auth.StudentLogIn"
            android:theme="@style/AppTheme.notitle">
        </activity>


        <activity android:name=".Auth.AdminLoginActivity"
            android:theme="@style/AppTheme.notitle">
        </activity>



        <activity android:name=".MainActivity"
                android:theme="@style/AppTheme.SplashFullScreen"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>